{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "dist",
        "declaration": true,
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "exclude": [
        "**/dist/**",
        "**/.output/**"
    ]
}
